<Window x:Class="ParsingCars.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ParsingCars"
        mc:Ignorable="d"
        Title="Cars" Height="350" Width="400">
    <Grid>
        <TextBox x:Name="searchInput" HorizontalAlignment="Left" Height="23" Margin="97,41,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="searchButton" Content="Search" HorizontalAlignment="Left" Margin="225,41,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click" Height="23"/>
        <ListView x:Name="searchResults" HorizontalAlignment="Left" Height="207" Margin="97,102,0,0" VerticalAlignment="Top" Width="203">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>
        <Label Content="Enter Make Below" HorizontalAlignment="Left" Margin="105,15,0,0" VerticalAlignment="Top"/>
        <Label Content="Model" HorizontalAlignment="Left" Margin="180,75,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
